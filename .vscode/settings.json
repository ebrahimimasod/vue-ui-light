{
	"vetur.validation.style": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true,
	},
	"eslint.validate": [
		"javascript",
		"typescript",
		"vue"
	],
	"editor.tabSize": 4,
	"editor.insertSpaces": false,
	"editor.detectIndentation": false,
	"vetur.format.options.tabSize": 4,
	"vetur.format.options.useTabs": true,
}